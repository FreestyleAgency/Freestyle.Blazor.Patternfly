<img class="pf-c-brand" src="@ImageUrl" alt="@AltText" @attributes="Attributes" />

@code {
	[Parameter]
	public string ImageUrl { get; set; } = "/css/assets/images/pf_logo.svg";

	[Parameter]
	public string AltText { get; set; } = "Company logo";

	[Parameter(CaptureUnmatchedValues = true)]
	public IDictionary<string, object> Attributes { get; set; }
}
